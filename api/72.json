        {
            "indexayat": "5474",
            "sourate": "72",
            "numeroaya": "27",
            "verset": "\u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0646\u0650 \u0627\u0631\u0652\u062a\u064e\u0636\u064e\u0649\u0670 \u0645\u0650\u0646 \u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064d \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f \u064a\u064e\u0633\u0652\u0644\u064f\u0643\u064f \u0645\u0650\u0646 \u0628\u064e\u064a\u0652\u0646\u0650 \u064a\u064e\u062f\u064e\u064a\u0652\u0647\u0650 \u0648\u064e\u0645\u0650\u0646\u0652 \u062e\u064e\u0644\u0652\u0641\u0650\u0647\u0650 \u0631\u064e\u0635\u064e\u062f\u064b\u0627",
            "urlhusary": "data\/Husary_64kbps\/072027.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/72_27.png"
        },
        {
            "indexayat": "5475",
            "sourate": "72",
            "numeroaya": "28",
            "verset": "\u0644\u0651\u0650\u064a\u064e\u0639\u0652\u0644\u064e\u0645\u064e \u0623\u064e\u0646 \u0642\u064e\u062f\u0652 \u0623\u064e\u0628\u0652\u0644\u064e\u063a\u064f\u0648\u0627 \u0631\u0650\u0633\u064e\u0627\u0644\u064e\u0627\u062a\u0650 \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u0652 \u0648\u064e\u0623\u064e\u062d\u064e\u0627\u0637\u064e \u0628\u0650\u0645\u064e\u0627 \u0644\u064e\u062f\u064e\u064a\u0652\u0647\u0650\u0645\u0652 \u0648\u064e\u0623\u064e\u062d\u0652\u0635\u064e\u0649\u0670 \u0643\u064f\u0644\u0651\u064e \u0634\u064e\u064a\u0652\u0621\u064d \u0639\u064e\u062f\u064e\u062f\u064b\u0627",
