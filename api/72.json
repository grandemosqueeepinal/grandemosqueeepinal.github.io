        {
            "indexayat": "5473",
            "sourate": "72",
            "numeroaya": "26",
            "verset": "\u0639\u064e\u0627\u0644\u0650\u0645\u064f \u0627\u0644\u0652\u063a\u064e\u064a\u0652\u0628\u0650 \u0641\u064e\u0644\u064e\u0627 \u064a\u064f\u0638\u0652\u0647\u0650\u0631\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u063a\u064e\u064a\u0652\u0628\u0650\u0647\u0650 \u0623\u064e\u062d\u064e\u062f\u064b\u0627",
