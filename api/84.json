            "urlhusary": "data\/Husary_64kbps\/084014.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_14.png"
        },
        {
            "indexayat": "5899",
            "sourate": "84",
            "numeroaya": "15",
