            "urlhusary": "data\/Husary_64kbps\/084015.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_15.png"
        },
        {
            "indexayat": "5900",
            "sourate": "84",
            "numeroaya": "16",
            "verset": "Non!... Je jure par le cr&eacute;puscule,",
            "urlhusary": "data\/Husary_64kbps\/084016.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_16.png"
        },
        {
            "indexayat": "5901",
            "sourate": "84",
            "numeroaya": "17",
