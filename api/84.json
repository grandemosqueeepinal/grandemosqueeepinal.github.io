    "FR": [
        {
            "indexayat": "5885",
            "sourate": "84",
            "numeroaya": "1",
            "verset": "Quand le ciel se d&eacute;chirera",
            "urlhusary": "data\/Husary_64kbps\/084001.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_1.png"
        },
        {
            "indexayat": "5886",
            "sourate": "84",
            "numeroaya": "2",
            "verset": "et ob&eacute;ira &agrave; son Seigneur - et fera ce qu&apos;il doit faire -",
            "urlhusary": "data\/Husary_64kbps\/084002.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_2.png"
        },
        {
            "indexayat": "5887",
            "sourate": "84",
            "numeroaya": "3",
            "verset": "et que la terre sera nivel&eacute;e,",
            "urlhusary": "data\/Husary_64kbps\/084003.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_3.png"
        },
        {
            "indexayat": "5888",
            "sourate": "84",
            "numeroaya": "4",
            "verset": "et qu&apos;elle rejettera ce qui est en son sein (les morts) et se videra,",
            "urlhusary": "data\/Husary_64kbps\/084004.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_4.png"
        },
        {
            "indexayat": "5889",
            "sourate": "84",
            "numeroaya": "5",
            "verset": "et qu&apos;elle ob&eacute;ira &agrave; son Seigneur - et fera ce qu&apos;elle doit faire -",
            "urlhusary": "data\/Husary_64kbps\/084005.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_5.png"
        },
        {
            "indexayat": "5890",
            "sourate": "84",
            "numeroaya": "6",
            "verset": "O homme! Toi qui t&apos;efforces vers ton Seigneur sans rel&acirc;che, tu Le rencontreras alors.",
            "urlhusary": "data\/Husary_64kbps\/084006.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_6.png"
        },
        {
            "indexayat": "5891",
            "sourate": "84",
            "numeroaya": "7",
            "verset": "Celui qui recevra son livre en sa main droite,",
            "urlhusary": "data\/Husary_64kbps\/084007.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_7.png"
        },
        {
            "indexayat": "5892",
            "sourate": "84",
            "numeroaya": "8",
            "verset": "sera soumis &agrave; un jugement facile,",
            "urlhusary": "data\/Husary_64kbps\/084008.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_8.png"
        },
        {
            "indexayat": "5893",
            "sourate": "84",
            "numeroaya": "9",
            "verset": "et retournera r&eacute;joui aupr&egrave;s de sa famille",
            "urlhusary": "data\/Husary_64kbps\/084009.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_9.png"
        },
        {
            "indexayat": "5894",
            "sourate": "84",
            "numeroaya": "10",
            "verset": "Quant &agrave; celui qui recevra son livre derri&egrave;re son dos,",
            "urlhusary": "data\/Husary_64kbps\/084010.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_10.png"
        },
        {
            "indexayat": "5895",
            "sourate": "84",
            "numeroaya": "11",
            "verset": "il invoquera la destruction sur lui-m&ecirc;me,",
            "urlhusary": "data\/Husary_64kbps\/084011.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_11.png"
        },
        {
            "indexayat": "5896",
            "sourate": "84",
            "numeroaya": "12",
            "verset": "et il br&ucirc;lera dans un feu ardent.",
            "urlhusary": "data\/Husary_64kbps\/084012.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/84_12.png"
        },
        {
            "indexayat": "5897",
            "sourate": "84",
            "numeroaya": "13",
            "verset": "Car il &eacute;tait tout joyeux parmi les siens,",
            "urlhusary": "data\/Husary_64kbps\/084013.mp3",
