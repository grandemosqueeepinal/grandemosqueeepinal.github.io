        {
            "indexayat": "5755",
            "sourate": "79",
            "numeroaya": "43",
            "verset": "\u0641\u0650\u064a\u0645\u064e \u0623\u064e\u0646\u062a\u064e \u0645\u0650\u0646 \u0630\u0650\u0643\u0652\u0631\u064e\u0627\u0647\u064e\u0627",
