        {
            "indexayat": "5756",
            "sourate": "79",
            "numeroaya": "44",
            "verset": "\u0625\u0650\u0644\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0645\u064f\u0646\u062a\u064e\u0647\u064e\u0627\u0647\u064e\u0627",
