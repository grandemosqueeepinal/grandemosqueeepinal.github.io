        },
        {
            "indexayat": "5757",
            "sourate": "79",
            "numeroaya": "45",
            "verset": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0623\u064e\u0646\u062a\u064e \u0645\u064f\u0646\u0630\u0650\u0631\u064f \u0645\u064e\u0646 \u064a\u064e\u062e\u0652\u0634\u064e\u0627\u0647\u064e\u0627",
