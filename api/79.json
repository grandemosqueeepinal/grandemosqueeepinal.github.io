        {
            "indexayat": "5758",
            "sourate": "79",
            "numeroaya": "46",
            "verset": "\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u0652 \u064a\u064e\u0648\u0652\u0645\u064e \u064a\u064e\u0631\u064e\u0648\u0652\u0646\u064e\u0647\u064e\u0627 \u0644\u064e\u0645\u0652 \u064a\u064e\u0644\u0652\u0628\u064e\u062b\u064f\u0648\u0627 \u0625\u0650\u0644\u0651\u064e\u0627 \u0639\u064e\u0634\u0650\u064a\u0651\u064e\u0629\u064b \u0623\u064e\u0648\u0652 \u0636\u064f\u062d\u064e\u0627\u0647\u064e\u0627",
