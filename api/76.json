        {
            "indexayat": "5622",
            "sourate": "76",
            "numeroaya": "31",
            "verset": "\u064a\u064f\u062f\u0652\u062e\u0650\u0644\u064f \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0621\u064f \u0641\u0650\u064a \u0631\u064e\u062d\u0652\u0645\u064e\u062a\u0650\u0647\u0650 \u0648\u064e\u0627\u0644\u0638\u0651\u064e\u0627\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0623\u064e\u0639\u064e\u062f\u0651\u064e \u0644\u064e\u0647\u064f\u0645\u0652 \u0639\u064e\u0630\u064e\u0627\u0628\u064b\u0627 \u0623\u064e\u0644\u0650\u064a\u0645\u064b\u0627",
