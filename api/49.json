            "verset": "\u064a\u064e\u0627 \u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0627\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0622\u0645\u064e\u0646\u064f\u0648\u0627 \u0627\u062c\u0652\u062a\u064e\u0646\u0650\u0628\u064f\u0648\u0627 \u0643\u064e\u062b\u0650\u064a\u0631\u064b\u0627 \u0645\u0651\u0650\u0646\u064e \u0627\u0644\u0638\u0651\u064e\u0646\u0651\u0650 \u0625\u0650\u0646\u0651\u064e \u0628\u064e\u0639\u0652\u0636\u064e \u0627\u0644\u0638\u0651\u064e\u0646\u0651\u0650 \u0625\u0650\u062b\u0652\u0645\u064c \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u062c\u064e\u0633\u0651\u064e\u0633\u064f\u0648\u0627 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u063a\u0652\u062a\u064e\u0628 \u0628\u0651\u064e\u0639\u0652\u0636\u064f\u0643\u064f\u0645 \u0628\u064e\u0639\u0652\u0636\u064b\u0627 \u0623\u064e\u064a\u064f\u062d\u0650\u0628\u0651\u064f \u0623\u064e\u062d\u064e\u062f\u064f\u0643\u064f\u0645\u0652 \u0623\u064e\u0646 \u064a\u064e\u0623\u0652\u0643\u064f\u0644\u064e \u0644\u064e\u062d\u0652\u0645\u064e \u0623\u064e\u062e\u0650\u064a\u0647\u0650 \u0645\u064e\u064a\u0652\u062a\u064b\u0627 \u0641\u064e\u0643\u064e\u0631\u0650\u0647\u0652\u062a\u064f\u0645\u064f\u0648\u0647\u064f \u0648\u064e\u0627\u062a\u0651\u064e\u0642\u064f\u0648\u0627 \u0627\u0644\u0644\u0651\u064e\u0647\u064e \u0625\u0650\u0646\u0651\u064e \u0627\u0644\u0644\u0651\u064e\u0647\u064e \u062a\u064e\u0648\u0651\u064e\u0627\u0628\u064c \u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064c",
