    "FR": [
        {
            "indexayat": "5949",
            "sourate": "87",
            "numeroaya": "1",
            "verset": "Glorifie le nom de ton Seigneur, le Tr&egrave;s Haut,",
            "urlhusary": "data\/Husary_64kbps\/087001.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_1.png"
        },
        {
            "indexayat": "5950",
            "sourate": "87",
            "numeroaya": "2",
            "verset": "Celui Qui a cr&eacute;&eacute; et agenc&eacute; harmonieusement,",
            "urlhusary": "data\/Husary_64kbps\/087002.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_2.png"
        },
        {
            "indexayat": "5951",
            "sourate": "87",
            "numeroaya": "3",
            "verset": "qui a d&eacute;cr&eacute;t&eacute; et guid&eacute;,",
            "urlhusary": "data\/Husary_64kbps\/087003.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_3.png"
        },
        {
            "indexayat": "5952",
            "sourate": "87",
            "numeroaya": "4",
            "verset": "et qui a fait pousser le p&acirc;turage,",
            "urlhusary": "data\/Husary_64kbps\/087004.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_4.png"
        },
        {
            "indexayat": "5953",
            "sourate": "87",
            "numeroaya": "5",
            "verset": "et en a fait ensuite un foin sombre.",
            "urlhusary": "data\/Husary_64kbps\/087005.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_5.png"
        },
        {
            "indexayat": "5954",
            "sourate": "87",
            "numeroaya": "6",
            "verset": "Nous te ferons r&eacute;citer (le Coran), de sorte que tu n&apos;oublieras",
            "urlhusary": "data\/Husary_64kbps\/087006.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_6.png"
        },
        {
            "indexayat": "5955",
            "sourate": "87",
            "numeroaya": "7",
            "verset": "que ce qu&apos;Allah veut. Car, Il conna&icirc;t ce qui para&icirc;t au grand jour ainsi que ce qui est cach&eacute;.",
            "urlhusary": "data\/Husary_64kbps\/087007.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_7.png"
        },
        {
            "indexayat": "5956",
            "sourate": "87",
            "numeroaya": "8",
            "verset": "Nous te mettrons sur la voie la plus facile.",
            "urlhusary": "data\/Husary_64kbps\/087008.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_8.png"
        },
        {
            "indexayat": "5957",
            "sourate": "87",
            "numeroaya": "9",
            "verset": "Rappelle, donc, o&ugrave; le Rappel doit &ecirc;tre utile.",
            "urlhusary": "data\/Husary_64kbps\/087009.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_9.png"
        },
        {
            "indexayat": "5958",
            "sourate": "87",
            "numeroaya": "10",
            "verset": "Quiconque craint (Allah) s&apos;[en] rappellera,",
            "urlhusary": "data\/Husary_64kbps\/087010.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_10.png"
        },
        {
            "indexayat": "5959",
            "sourate": "87",
            "numeroaya": "11",
            "verset": "et s&apos;en &eacute;cartera le grand malheureux,",
            "urlhusary": "data\/Husary_64kbps\/087011.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_11.png"
        },
        {
            "indexayat": "5960",
            "sourate": "87",
            "numeroaya": "12",
            "verset": "qui br&ucirc;lera dans le plus grand Feu,",
            "urlhusary": "data\/Husary_64kbps\/087012.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_12.png"
        },
        {
            "indexayat": "5961",
            "sourate": "87",
            "numeroaya": "13",
            "verset": "o&ugrave; il ne mourra ni ne vivra.",
            "urlhusary": "data\/Husary_64kbps\/087013.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_13.png"
        },
        {
            "indexayat": "5962",
            "sourate": "87",
            "numeroaya": "14",
            "verset": "R&eacute;ussit, certes, celui qui se purifie,",
            "urlhusary": "data\/Husary_64kbps\/087014.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_14.png"
        },
        {
            "indexayat": "5963",
            "sourate": "87",
            "numeroaya": "15",
            "verset": "et se rappelle le nom de son Seigneur, puis c&eacute;l&egrave;bre la Sal&acirc;t.",
            "urlhusary": "data\/Husary_64kbps\/087015.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_15.png"
        },
        {
            "indexayat": "5964",
            "sourate": "87",
            "numeroaya": "16",
            "verset": "Mais, vous pr&eacute;f&eacute;rez plut&ocirc;t la vie pr&eacute;sente,",
            "urlhusary": "data\/Husary_64kbps\/087016.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_16.png"
        },
        {
            "indexayat": "5965",
            "sourate": "87",
            "numeroaya": "17",
            "verset": "alors que l&apos;au-del&agrave; est meilleur et plus durable.",
            "urlhusary": "data\/Husary_64kbps\/087017.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_17.png"
        },
        {
            "indexayat": "5966",
            "sourate": "87",
            "numeroaya": "18",
            "verset": "Ceci se trouve, certes, dans les Feuilles anciennes,",
            "urlhusary": "data\/Husary_64kbps\/087018.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_18.png"
        },
        {
            "indexayat": "5967",
            "sourate": "87",
            "numeroaya": "19",
            "verset": "les Feuilles d&apos;Abraham et de Mo&iuml;se.",
            "urlhusary": "data\/Husary_64kbps\/087019.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/87_19.png"
        }
    ],
