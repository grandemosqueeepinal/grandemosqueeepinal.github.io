            "indexayat": "4504",
            "sourate": "45",
            "numeroaya": "31",
            "verset": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627 \u0627\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627 \u0623\u064e\u0641\u064e\u0644\u064e\u0645\u0652 \u062a\u064e\u0643\u064f\u0646\u0652 \u0622\u064a\u064e\u0627\u062a\u0650\u064a \u062a\u064f\u062a\u0652\u0644\u064e\u0649\u0670 \u0639\u064e\u0644\u064e\u064a\u0652\u0643\u064f\u0645\u0652 \u0641\u064e\u0627\u0633\u0652\u062a\u064e\u0643\u0652\u0628\u064e\u0631\u0652\u062a\u064f\u0645\u0652 \u0648\u064e\u0643\u064f\u0646\u062a\u064f\u0645\u0652 \u0642\u064e\u0648\u0652\u0645\u064b\u0627 \u0645\u0651\u064f\u062c\u0652\u0631\u0650\u0645\u0650\u064a\u0646\u064e",
            "urlhusary": "data\/Husary_64kbps\/045031.mp3",
