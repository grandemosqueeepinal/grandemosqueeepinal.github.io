        },
        {
            "indexayat": "4505",
            "sourate": "45",
            "numeroaya": "32",
            "verset": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627 \u0642\u0650\u064a\u0644\u064e \u0625\u0650\u0646\u0651\u064e \u0648\u064e\u0639\u0652\u062f\u064e \u0627\u0644\u0644\u0651\u064e\u0647\u0650 \u062d\u064e\u0642\u0651\u064c \u0648\u064e\u0627\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u064f \u0644\u064e\u0627 \u0631\u064e\u064a\u0652\u0628\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u0642\u064f\u0644\u0652\u062a\u064f\u0645 \u0645\u0651\u064e\u0627 \u0646\u064e\u062f\u0652\u0631\u0650\u064a \u0645\u064e\u0627 \u0627\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u064f \u0625\u0650\u0646 \u0646\u0651\u064e\u0638\u064f\u0646\u0651\u064f \u0625\u0650\u0644\u0651\u064e\u0627 \u0638\u064e\u0646\u0651\u064b\u0627 \u0648\u064e\u0645\u064e\u0627 \u0646\u064e\u062d\u0652\u0646\u064f \u0628\u0650\u0645\u064f\u0633\u0652\u062a\u064e\u064a\u0652\u0642\u0650\u0646\u0650\u064a\u0646\u064e",
