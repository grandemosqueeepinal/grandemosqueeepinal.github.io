        {
            "indexayat": "4506",
            "sourate": "45",
            "numeroaya": "33",
            "verset": "\u0648\u064e\u0628\u064e\u062f\u064e\u0627 \u0644\u064e\u0647\u064f\u0645\u0652 \u0633\u064e\u064a\u0651\u0650\u0626\u064e\u0627\u062a\u064f \u0645\u064e\u0627 \u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627 \u0648\u064e\u062d\u064e\u0627\u0642\u064e \u0628\u0650\u0647\u0650\u0645 \u0645\u0651\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627 \u0628\u0650\u0647\u0650 \u064a\u064e\u0633\u0652\u062a\u064e\u0647\u0652\u0632\u0650\u0626\u064f\u0648\u0646\u064e",
            "urlhusary": "data\/Husary_64kbps\/045033.mp3",
