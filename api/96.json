    "FR": [
        {
            "indexayat": "6107",
            "sourate": "96",
            "numeroaya": "1",
            "verset": "Lis, au nom de ton Seigneur qui a cr&eacute;&eacute;,",
            "urlhusary": "data\/Husary_64kbps\/096001.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_1.png"
        },
        {
            "indexayat": "6108",
            "sourate": "96",
            "numeroaya": "2",
            "verset": "qui a cr&eacute;&eacute; l&apos;homme d&apos;une adh&eacute;rence.",
            "urlhusary": "data\/Husary_64kbps\/096002.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_2.png"
        },
        {
            "indexayat": "6109",
            "sourate": "96",
            "numeroaya": "3",
            "verset": "Lis! Ton Seigneur est le Tr&egrave;s Noble,",
            "urlhusary": "data\/Husary_64kbps\/096003.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_3.png"
        },
        {
            "indexayat": "6110",
            "sourate": "96",
            "numeroaya": "4",
            "verset": "qui a enseign&eacute; par la plume [le calame],",
            "urlhusary": "data\/Husary_64kbps\/096004.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_4.png"
        },
        {
            "indexayat": "6111",
            "sourate": "96",
            "numeroaya": "5",
            "verset": "a enseign&eacute; &agrave; l&apos;homme ce qu&apos;il ne savait pas.",
            "urlhusary": "data\/Husary_64kbps\/096005.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_5.png"
        },
        {
            "indexayat": "6112",
            "sourate": "96",
            "numeroaya": "6",
            "verset": "Prenez-garde! Vraiment l&apos;homme devient rebelle,",
            "urlhusary": "data\/Husary_64kbps\/096006.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_6.png"
        },
        {
            "indexayat": "6113",
            "sourate": "96",
            "numeroaya": "7",
            "verset": "d&egrave;s qu&apos;il estime qu&apos;il peut se suffire &agrave; lui-m&ecirc;me (&agrave; cause de sa richesse).",
            "urlhusary": "data\/Husary_64kbps\/096007.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_7.png"
        },
        {
            "indexayat": "6114",
            "sourate": "96",
            "numeroaya": "8",
            "verset": "Mais, c&apos;est vers ton Seigneur qu&apos;est le retour.",
            "urlhusary": "data\/Husary_64kbps\/096008.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_8.png"
        },
        {
            "indexayat": "6115",
            "sourate": "96",
            "numeroaya": "9",
            "verset": "As-tu vu celui qui interdit",
            "urlhusary": "data\/Husary_64kbps\/096009.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_9.png"
        },
        {
            "indexayat": "6116",
            "sourate": "96",
            "numeroaya": "10",
            "verset": "&agrave; un serviteur d&apos;Allah [Muhammad] de c&eacute;l&eacute;brer la Sal&acirc;t?",
            "urlhusary": "data\/Husary_64kbps\/096010.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_10.png"
        },
        {
            "indexayat": "6117",
            "sourate": "96",
            "numeroaya": "11",
            "verset": "Vois-tu s&apos;il est sur la bonne voie,",
            "urlhusary": "data\/Husary_64kbps\/096011.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_11.png"
        },
        {
            "indexayat": "6118",
            "sourate": "96",
            "numeroaya": "12",
            "verset": "ou s&apos;il ordonne la pi&eacute;t&eacute;?",
            "urlhusary": "data\/Husary_64kbps\/096012.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_12.png"
        },
        {
            "indexayat": "6119",
            "sourate": "96",
            "numeroaya": "13",
            "verset": "Vois-tu s&apos;il d&eacute;ment et tourne le dos?",
            "urlhusary": "data\/Husary_64kbps\/096013.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_13.png"
        },
        {
            "indexayat": "6120",
            "sourate": "96",
            "numeroaya": "14",
            "verset": "Ne sait-il pas que vraiment Allah voit?",
            "urlhusary": "data\/Husary_64kbps\/096014.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_14.png"
        },
        {
            "indexayat": "6121",
            "sourate": "96",
            "numeroaya": "15",
            "verset": "Mais non! S&apos;il ne cesse pas, Nous le saisirons certes, par le toupet,",
            "urlhusary": "data\/Husary_64kbps\/096015.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_15.png"
        },
        {
            "indexayat": "6122",
            "sourate": "96",
            "numeroaya": "16",
            "verset": "le toupet d&apos;un menteur, d&apos;un p&eacute;cheur.",
            "urlhusary": "data\/Husary_64kbps\/096016.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_16.png"
        },
        {
            "indexayat": "6123",
            "sourate": "96",
            "numeroaya": "17",
            "verset": "Qu&apos;il appelle donc son assembl&eacute;e.",
            "urlhusary": "data\/Husary_64kbps\/096017.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_17.png"
        },
        {
            "indexayat": "6124",
            "sourate": "96",
            "numeroaya": "18",
            "verset": "Nous appellerons les gardiens [de l&apos;Enfer].",
            "urlhusary": "data\/Husary_64kbps\/096018.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_18.png"
        },
        {
            "indexayat": "6125",
            "sourate": "96",
            "numeroaya": "19",
            "verset": "Non! Ne lui ob&eacute;is pas; mais prosterne-toi et rapproche-toi.",
            "urlhusary": "data\/Husary_64kbps\/096019.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/96_19.png"
        }
    ],
