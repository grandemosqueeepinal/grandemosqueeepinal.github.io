        },
        {
            "indexayat": "5546",
            "sourate": "74",
            "numeroaya": "51",
            "verset": "s&apos;enfuyant devant un lion.",
            "urlhusary": "data\/Husary_64kbps\/074051.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/74_51.png"
        },
        {
            "indexayat": "5547",
            "sourate": "74",
            "numeroaya": "52",
            "verset": "Chacun d&apos;eux voudrait plut&ocirc;t qu&apos;on lui apporte des feuilles tout &eacute;tal&eacute;es.",
            "urlhusary": "data\/Husary_64kbps\/074052.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/74_52.png"
        },
        {
            "indexayat": "5548",
            "sourate": "74",
            "numeroaya": "53",
            "verset": "Ah! Non! C&apos;est plut&ocirc;t qu&apos;ils ne craignent pas l&apos;au-del&agrave;.",
            "urlhusary": "data\/Husary_64kbps\/074053.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/74_53.png"
        },
        {
            "indexayat": "5549",
            "sourate": "74",
            "numeroaya": "54",
            "verset": "Ah! Non! Ceci est vraiment un Rappel.",
            "urlhusary": "data\/Husary_64kbps\/074054.mp3",
