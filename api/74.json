            "imghigh": "api\/hadith\/ayatimages\/high\/74_37.png"
        },
        {
            "indexayat": "5533",
            "sourate": "74",
            "numeroaya": "38",
            "verset": "Toute &acirc;me est l&apos;otage de ce qu&apos;elle a acquis.",
            "urlhusary": "data\/Husary_64kbps\/074038.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/74_38.png"
        },
        {
            "indexayat": "5534",
            "sourate": "74",
            "numeroaya": "39",
            "verset": "Sauf les gens de la droite (les &eacute;lus):",
            "urlhusary": "data\/Husary_64kbps\/074039.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/74_39.png"
        },
        {
            "indexayat": "5535",
            "sourate": "74",
            "numeroaya": "40",
            "verset": "dans des Jardins, ils s&apos;interrogeront",
            "urlhusary": "data\/Husary_64kbps\/074040.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/74_40.png"
        },
        {
            "indexayat": "5536",
            "sourate": "74",
            "numeroaya": "41",
            "verset": "au sujet des criminels:",
            "urlhusary": "data\/Husary_64kbps\/074041.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/74_41.png"
        },
        {
            "indexayat": "5537",
            "sourate": "74",
            "numeroaya": "42",
