            "urlhusary": "data\/Husary_64kbps\/074042.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/74_42.png"
        },
        {
            "indexayat": "5538",
            "sourate": "74",
            "numeroaya": "43",
