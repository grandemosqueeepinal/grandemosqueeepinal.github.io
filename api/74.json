            "imghigh": "api\/hadith\/ayatimages\/high\/74_44.png"
        },
        {
            "indexayat": "5540",
            "sourate": "74",
            "numeroaya": "45",
            "verset": "et nous nous associions &agrave; ceux qui tenaient des conversations futiles,",
            "urlhusary": "data\/Husary_64kbps\/074045.mp3",
