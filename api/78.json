    "FR": [
        {
            "indexayat": "5673",
            "sourate": "78",
            "numeroaya": "1",
            "verset": "Sur quoi s&apos;interrogent-ils mutuellement?",
            "urlhusary": "data\/Husary_64kbps\/078001.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_1.png"
        },
        {
            "indexayat": "5674",
            "sourate": "78",
            "numeroaya": "2",
            "verset": "Sur la grande nouvelle,",
            "urlhusary": "data\/Husary_64kbps\/078002.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_2.png"
        },
        {
            "indexayat": "5675",
            "sourate": "78",
            "numeroaya": "3",
            "verset": "&agrave; propos de laquelle ils divergent.",
            "urlhusary": "data\/Husary_64kbps\/078003.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_3.png"
        },
        {
            "indexayat": "5676",
            "sourate": "78",
            "numeroaya": "4",
            "verset": "Eh bien non! Ils sauront bient&ocirc;t.",
            "urlhusary": "data\/Husary_64kbps\/078004.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_4.png"
        },
        {
            "indexayat": "5677",
            "sourate": "78",
            "numeroaya": "5",
            "verset": "Encore une fois, non! Ils sauront bient&ocirc;t.",
            "urlhusary": "data\/Husary_64kbps\/078005.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_5.png"
        },
        {
            "indexayat": "5678",
            "sourate": "78",
            "numeroaya": "6",
            "verset": "N&apos;avons-Nous pas fait de la terre une couche?",
            "urlhusary": "data\/Husary_64kbps\/078006.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_6.png"
        },
        {
            "indexayat": "5679",
            "sourate": "78",
            "numeroaya": "7",
            "verset": "et (plac&eacute;) les montagnes comme des piquets?",
            "urlhusary": "data\/Husary_64kbps\/078007.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_7.png"
        },
        {
            "indexayat": "5680",
            "sourate": "78",
            "numeroaya": "8",
            "verset": "Nous vous avons cr&eacute;&eacute;s en couples,",
            "urlhusary": "data\/Husary_64kbps\/078008.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_8.png"
        },
        {
            "indexayat": "5681",
            "sourate": "78",
            "numeroaya": "9",
            "verset": "et d&eacute;sign&eacute; votre sommeil pour votre repos,",
            "urlhusary": "data\/Husary_64kbps\/078009.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_9.png"
        },
        {
            "indexayat": "5682",
            "sourate": "78",
            "numeroaya": "10",
            "verset": "et fait de la nuit un v&ecirc;tement,",
            "urlhusary": "data\/Husary_64kbps\/078010.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_10.png"
        },
        {
            "indexayat": "5683",
            "sourate": "78",
            "numeroaya": "11",
            "verset": "et assign&eacute; le jour pour les affaires de la vie,",
            "urlhusary": "data\/Husary_64kbps\/078011.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_11.png"
        },
        {
            "indexayat": "5684",
            "sourate": "78",
            "numeroaya": "12",
            "verset": "et construit au-dessus de vous sept (cieux) renforc&eacute;s,",
            "urlhusary": "data\/Husary_64kbps\/078012.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_12.png"
        },
        {
            "indexayat": "5685",
            "sourate": "78",
            "numeroaya": "13",
            "verset": "et [y] avons plac&eacute; une lampe (le soleil) tr&egrave;s ardente,",
            "urlhusary": "data\/Husary_64kbps\/078013.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_13.png"
        },
        {
            "indexayat": "5686",
            "sourate": "78",
            "numeroaya": "14",
            "verset": "et fait descendre des nu&eacute;es une eau abondante",
