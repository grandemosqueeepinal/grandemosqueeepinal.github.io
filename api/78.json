        },
        {
            "indexayat": "5692",
            "sourate": "78",
            "numeroaya": "20",
            "verset": "et les montagnes seront mises en marche et deviendront un mirage.",
            "urlhusary": "data\/Husary_64kbps\/078020.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_20.png"
        },
        {
            "indexayat": "5693",
            "sourate": "78",
            "numeroaya": "21",
            "verset": "L&apos;Enfer demeure aux aguets,",
            "urlhusary": "data\/Husary_64kbps\/078021.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_21.png"
        },
        {
            "indexayat": "5694",
            "sourate": "78",
            "numeroaya": "22",
            "verset": "refuge pour les transgresseurs.",
            "urlhusary": "data\/Husary_64kbps\/078022.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_22.png"
        },
        {
            "indexayat": "5695",
            "sourate": "78",
            "numeroaya": "23",
            "verset": "Ils y demeureront pendant des si&egrave;cles successifs.",
            "urlhusary": "data\/Husary_64kbps\/078023.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_23.png"
        },
        {
            "indexayat": "5696",
            "sourate": "78",
            "numeroaya": "24",
            "verset": "Ils n&apos;y go&ucirc;teront ni fra&icirc;cheur ni breuvage,",
            "urlhusary": "data\/Husary_64kbps\/078024.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_24.png"
        },
        {
            "indexayat": "5697",
            "sourate": "78",
            "numeroaya": "25",
            "verset": "Hormis une eau bouillante et un pus",
