            "imghigh": "api\/hadith\/ayatimages\/high\/78_25.png"
        },
        {
            "indexayat": "5698",
            "sourate": "78",
            "numeroaya": "26",
            "verset": "comme r&eacute;tribution &eacute;quitable.",
            "urlhusary": "data\/Husary_64kbps\/078026.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_26.png"
        },
        {
            "indexayat": "5699",
            "sourate": "78",
            "numeroaya": "27",
            "verset": "Car ils ne s&apos;attendaient pas &agrave; rendre compte,",
            "urlhusary": "data\/Husary_64kbps\/078027.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_27.png"
        },
        {
            "indexayat": "5700",
            "sourate": "78",
            "numeroaya": "28",
            "verset": "et traitaient de mensonges, continuellement, Nos versets,",
            "urlhusary": "data\/Husary_64kbps\/078028.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_28.png"
        },
        {
            "indexayat": "5701",
            "sourate": "78",
            "numeroaya": "29",
            "verset": "alors que Nous avons d&eacute;nombr&eacute; toutes choses en &eacute;crit.",
            "urlhusary": "data\/Husary_64kbps\/078029.mp3",
            "imghigh": "api\/hadith\/ayatimages\/high\/78_29.png"
        },
        {
            "indexayat": "5702",
            "sourate": "78",
            "numeroaya": "30",
            "verset": "Go&ucirc;tez-donc. Nous n&apos;augmenterons pour vous que le ch&acirc;timent!",
